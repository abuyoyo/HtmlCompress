{
    "name": "wyrihaximus/html-compress",
    "description": "Compress/minify your HTML",
    "keywords": [
        "html",
        "compress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4",
        "voku/html-min": "^4.1",
        "wyrihaximus/compress": "^1.1",
        "wyrihaximus/css-compress": "^1.0.1",
        "wyrihaximus/js-compress": "^1.0"
    },
    "require-dev": {
        "wyrihaximus/coding-standard": "^1.0.0-alpha1",
        "wyrihaximus/test-utilities": "^2.0.0-alpha1"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "extra": {
        "unused": [
            "slevomat/coding-standard"
        ]
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\HtmlCompress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\HtmlCompress\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
